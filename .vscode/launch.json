{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug dts-linter",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/dts-linter.ts",
        "--format",
        "--outputType",
        "json",
        "--cwd",
        "/Users/kylebonnici/workspace/personal/zephyr/",
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
