{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug dts-linter",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/src/dts-linter.ts",
        "--format",

        "--outputFormat",
        "json",
        "--patchFile",
        "dts_linter.patch",
        "--file",
        "/Users/kylebonnici/workspace/personal/zephyr/samples/net/openthread/coprocessor/boards/tlsr9518adk80d.overlay",
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
